@import url(https://fonts.googleapis.com/css?family=Raleway);

body {

  color:#414141 !important;
  font-family: Helvetica, Arial, Sans-Serif !important;
}

body>.container-fluid{
	padding-top: 100px;
}

nav {
  margin-top: 1px;
  padding: 15px 15px 0px 15px;
  text-align: left;
  font-family: Raleway;
  box-shadow: 2px 2px 8px rgba(0, 0, 0, 0.5);
}

#nav-3 {
  background: #ffffff;
}

.padding
{
  padding: 0 10px;
  margin: 0 10px;
}
.link-3 {
  transition: 0.4s;
  color: #694ed6;
  font-size: 20px;
  text-decoration: none;
}
.link-3:hover {
  background-color: #ffffff;
  color:  #cb42ab;
}

.homelink, .logoutlink {
  transition: 0.4s;
  color:  #694ed6;
  font-size: 25px;
}


.homelink:hover, .logoutlink:hover {
	color:  #cb42ab;
}


.logoutlink {
	padding-right: 40px;
}

.nav-image{
	width: 6.5pc;
	margin-top: -25px;
}

.divider-vertical {
	height: 50px !important;
	margin: 0 9px;
	border-left: 2px solid #cb42ab;
	border-right: 2px solid #ffffff;
}

.divider-horizontal {
	border-top: 2px solid #cb42ab;
	border-radius: 2px;
}

.nav-tabs {
    border-bottom: none !important;
}

.side-bar-nav{
	padding-bottom: 15px;
}

.navbar-nav>li>a
{
	color: #cb42ab;
	margin-top: 1px;
	transition: 0.7s;
	width: 11.5pc;
}

.navbar-nav>li>a:hover, .navbar-nav>li>a:focus
{
	color: #ffffff !important;
	border-radius: 1px !important;
	background-color: #694ed6 !important;
	width: 11.5pc;
}

hr {
    display: block;
    border: 0;
    border-top: 1px solid #ccc;
    margin-top: 0px !important;
    border-color: #cb42ab !important;
    background-color: #cb42ab;
}

.panel-primary {
    border-color: #694ed6 !important;
}
/* 
.panel-default>.panel-heading
{
	background-color: #b4a7eb !important;
	color:#ffffff !important;
} */
.panel-primary>.panel-heading
{
	background-color: #694ed6 !important;
	border-color: #694ed6 !important;
}

.form-control:focus{
	border-color: #694ed6 !important;
}

input[type="text"] {
  border: 1px solid #b4b4b4;
  margin: 0px;
  -moz-transition: all .5s;
  -webkit-transition: all .5s;
  transition: all .5s;
}

input[type="text"]:focus, input[type="password"]:focus {
  border-color: #694ed6;
  outline: 0;
  -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(102, 175, 233, 0.6);
  box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(102, 175, 233, 0.6);
  
}


input[type="password"] {
  margin-bottom: 20px;
  border: 1px solid #b4b4b4;
  margin: 0px;
  -moz-transition: all .5s;
  -webkit-transition: all .5s;
  transition: all .5s;
}

.column{
	border: 1px solid #cecece;
    background-color: #ededed;
    border-radius: 15px;
}

.marginLeft15px{
    margin-left: 15px !important;
}

.marginBtm15px{
	margin-bottom: 15px !important;
}

.plaintext {
  display: block;
  width: 100%;
  padding-top: 0.375rem;
  padding-bottom: 0.375rem;
  margin-bottom: 0;
  line-height: 1.5;
  box-shadow:none !important;
  background-color: transparent;
  border: 0px !important;
}

.width5pc
{
	width: 5pc;
}

.width10pc
{
	width: 10pc !important;
}

